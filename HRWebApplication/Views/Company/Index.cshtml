@model List<Company>
@{
    ViewData["Title"] = "Index";
}

<h1>List of all companies</h1>
<div>
    <table class="table table-striped table-hover">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Company</th>
                <th scope="col">Id</th>
            </tr>
        </thead>
        <tbody>
            @foreach (Company company in Model)
            {
                <tr>
                    <td>@company.Name</td>
                    <td>@company.Id</td>
                </tr>
            }
        </tbody>
    </table>
</div>